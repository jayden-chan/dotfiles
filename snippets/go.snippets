snippet func
	func ${1:}(${2:}) ${3:}{
		${0}
	}

snippet meth
	func (${1:}) ${2:}() ${3:}{
		${0}
	}

snippet pf
	fmt.Printf(${1})

snippet cw
	fmt.Println(${1})

snippet lf
	log.Printf(${1})

snippet lw
	log.Println(${1})

snippet imp
	"github.com/jayden-chan/${0}"

snippet for
	for ${1:} := 0; $1 < ${2:}; $1++ {
		
	}

snippet forr
	for ${1:} := ${2:}; $1 > 0; $1-- {
		
	}

snippet fore
	for ${1}, ${2} := range ${3} {
		
	}
